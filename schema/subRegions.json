{
  "type": "object",
  "description": "Mapping of sub-regions and their attributes within the super-region this file is named after.",
  "patternProperties": {
    "^[0-9]+$": {
      "type": "object",
      "description": "A sub-region.",
      "properties": {
        "longName": { "type": "string" },
        "shortName": { "type": "string" },
        "shapeRelativePath": { "type": "string" }
      },
      "required": ["longName", "shortName", "shapeRelativePath"],
      "additionalProperties": false
    }
  }
}
