{
  "type": "object",
  "description": "Mapping of sub-region collections and their attributes.",
  "patternProperties": {
    "^.*$": {
      "type": "object",
      "description": "A sub-region collection.",
      "properties": {
        "longName": { "type": "string" },
        "shortName": { "type": "string" }
      },
      "required": ["longName", "shortName"],
      "additionalProperties": false
    }
  }
}
